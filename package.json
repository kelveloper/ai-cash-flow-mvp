{
  "name": "ai-cash-flow-mvp",
  "version": "1.0.0",
  "description": "AI-powered cash flow analysis dashboard",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "build": "vite build",
    "preview": "vite preview",
    "build:css": "tailwindcss -i ./app/static/css/tailwind.css -o ./app/static/css/tailwind.min.css --watch",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^4.28.0",
    "@types/react-datepicker": "^6.2.0",
    "autoprefixer": "^10.4.17",
    "axios": "^1.3.4",
    "chart.js": "^4.2.1",
    "clsx": "^1.2.1",
    "concurrently": "^8.2.2",
    "date-fns": "^2.29.3",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "vite": "^4.1.0"
  }
}
